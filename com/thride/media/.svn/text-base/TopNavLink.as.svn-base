package com.thride.media{		import flash.display.MovieClip;	import flash.display.Sprite;		import flash.events.Event;		import flash.text.TextField;	import flash.text.TextFieldAutoSize;		import flash.geom.Rectangle;		import gs.TweenLite;	import gs.easing.*;	//TweenLite.to(mc, 1, {x:46, y:43, scaleX:1, scaleY:1, rotation:0, alpha:1, tint:0x3399ff, ease:Elastic.easeOut, onComplete:FunctionName });			public class TopNavLink extends MovieClip	{		private var navMargin:Number = 10;		public var index:Number;				public var id:String;		public var xmlsrc:String;		public var titleString:String;		public var swfsrc:String;				public var hit:Sprite;		private var txtmc:navtxt;		private var field:TextField;		public var slant:navslant;						public function TopNavLink($titleString, $idString)		{			titleString = $titleString;			id = $idString;			//addEventListener(Event.ADDED_TO_STAGE, onAdded);			init();		}				private function init()		{								txtmc = new navtxt();				field = txtmc.getChildByName("txt") as TextField;				slant = new navslant();								addChild(txtmc);				field.autoSize = TextFieldAutoSize.LEFT;				field.htmlText = titleString;				field.width = field.textWidth+5;								addChild(slant); 				slant.x = field.width + navMargin;								hit = new Sprite();				hit.graphics.beginFill(0x000000, 0);				hit.graphics.drawRect(-navMargin, -navMargin , (field.textWidth+(navMargin*2)), (field.textHeight+(navMargin*2)));				hit.graphics.endFill();				addChild(hit);								slant.x = hit.width;													}				public function overAnim(){			TweenLite.to(slant, 1, {tint:0x000000});			TweenLite.to(txtmc, .2, {tint:0xD60000, scaleX:1.1,scaleY:1.1, y:-2, x:-3, ease:Strong.easeOut });		}						public function outAnim(){			TweenLite.killTweensOf(txtmc);			TweenLite.killTweensOf(slant);			TweenLite.to(slant, 1, {tint:0xD60000});			TweenLite.to(txtmc, .2, {tint:0x666666, rotation:0, scaleX:1,scaleY:1, y:0, x:0, ease:Bounce.easeOut});		}				public function deadAnim(){			txtmc.alpha = .5;		}				private function onAdded(e:Event)		{						init();		}					}}