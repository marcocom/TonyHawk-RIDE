package com.thride{	import flash.events.Event;		public class NavEvent extends Event 	{		public static const LINKOUT:String = "linkOut";		public static const PHONELINK:String = "phoneOut";		public static var url:String;		public static var args:*;						public function NavEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false, ... a:*) 		{ 			super(type, bubbles, cancelable); //(type:String, bubbles:Boolean = false, cancelable:Boolean = false)			args = a;					} 				public function get values():Array		{			return args;		}		public override function clone():Event 		{ 			return new NavEvent(type, bubbles, cancelable, args);		} 		public override function toString():String 		{ 			return formatToString("NavEvent", "type", "values", "eventPhase"); 		}	}}