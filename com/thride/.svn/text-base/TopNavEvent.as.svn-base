package com.thride{	import flash.events.Event;		public class TopNavEvent extends Event 	{		public static const NAVCHOICE:String = "navChoice";		public static var _titleString:String;		public static var _idString:String;		public static var _xmlsrcString:String;		public static var _swfloc:String;		public static var args:*;				public function TopNavEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false, ... a:*) 		{ 			super(type, bubbles, cancelable); //(type:String, bubbles:Boolean = false, cancelable:Boolean = false)			args = a;						_idString = args[0];		} 		public function get idString():String		{			return _idString;		}				public function get values():Array		{			return args;		}		public override function clone():Event 		{ 			return new TopNavEvent(type, bubbles, cancelable, args);		} 		public override function toString():String 		{ 			return formatToString("TopNavEvent", "type", "values", "eventPhase"); 		}	}}